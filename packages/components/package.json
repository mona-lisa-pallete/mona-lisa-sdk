{
  "name": "@davinci/components",
  "version": "0.0.1",
  "description": "> TODO: description",
  "author": "陈炳瑞 <chenbingrui@guorou.tv>",
  "homepage": "",
  "license": "ISC",
  "module": "lib/davinciComponents.esm.js",
  "main": "lib/davinciComponents.dll.js",
  "sideEffects": [
    "*.scss",
    "*.css"
  ],
  "types": "./types/index.d.ts",
  "keywords": [
    "taro",
    "taro-ui",
    "ui"
  ],
  "files": [
    "lib",
    "dist",
    "types"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@git.shensz.local:davinci/davinci-sdk.git"
  },
  "scripts": {
    "dev": "yarn run dev:lib",
    "dev:lib": "tsc --project ./tsconfig.build.json --watch --incremental",
    "build": "yarn run build:rollup && yarn run build:lib",
    "build:lib": "tsc --project ./tsconfig.build.json",
    "build:rollup": "rollup --config ./config/rollup.config.js",
    "build:vendor": "webpack --config ./webpack.vendor.config.js && webpack --config ./webpack.config.js",
    "prepublishOnly": "yarn run clean && yarn run build",
    "lint": "eslint ./src --fix",
    "lint:style": "stylelint \"src/**/*.scss\" --syntax scss",
    "lint:style-fix": "stylelint \"src/**/*.scss\" --syntax scss --fix",
    "test": "cross-env NODE_ENV=test && jest --coverage",
    "test:ci": "yarn run build:h5 && yarn run test",
    "test:analyze": "rollup --config ./config/rollup.config.analyze.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean": "rimraf .temp dist lib coverage"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@davinci/core": "file:../core",
    "@tarojs/components": "3.2.8",
    "@tarojs/react": "3.2.8",
    "@tarojs/runtime": "3.2.8",
    "@tarojs/taro": "3.2.8",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "antd": "4.15.5"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/mini-runner": "3.2.8",
    "@tarojs/webpack-runner": "3.2.8",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-preset-taro": "3.2.8",
    "eslint": "^6.8.0",
    "eslint-config-taro": "3.2.8",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "serve": "^11.3.2",
    "stylelint": "9.3.0",
    "ts-loader": "^9.1.2",
    "typescript": "^4.1.0",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0"
  }
}
